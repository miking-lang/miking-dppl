function iftest(p: Real, q: Real) {
  coin1 = flip(p)
  coin2 = flip(q)

  if coin1 {
    if coin2 {
      // Case 1 is triggered, program ends
      weight(1)
      return()
      weight(2)
      return()
    }
    // Case 2 and then case 4 will follow
    weight(2)
  }
  else {
    // Case 3 and then case 4 will follow
    weight(3)
  }
  // Case 4 only
  weight(4)
}




