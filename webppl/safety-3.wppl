var a_sampler = function(a, b, p)
{
    if (flip(p))
    {
	return ((a + b)/2)
    } else {
	return a_sampler(a, b/2, p)
    }
}

var model = function()
{
    var a = a_sampler(0,1, 0.001)

    console.log(a)

    var t = exponential(
	{
	    a: a
	}
    )

    return t
}

var dist = Infer(
    {
	method: "SMC",
	particles: 10000,
	model: model
    }
)

dist.normalizationConstant
